{
  "manifest_version": 3,
  "name": "__MSG_name__",
  "version": "1.0.0",
  "description": "__MSG_description__",
  "icons": {
    "128": "images/main-icon128.png",
    "48": "images/main-icon48.png",
    "16": "images/main-icon16.png"
  },
  "default_locale": "en",
  "action": {
    "default_icon": {
      "128": "images/main-icon128.png",
      "48": "images/main-icon48.png",
      "16": "images/main-icon16.png"
    },
    "default_title": "EzTab",
    "default_popup": "src/popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "saveUrl": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Command+Shift+S"
      },
      "description": "Save Current Url"
    }
  },
  "permissions": [
    "history",
    "tabs",
    "contextMenus",
    "storage",
    "notifications",
    "search"
  ],
  "host_permissions": [
    "chrome://favicon/"
  ]
}
